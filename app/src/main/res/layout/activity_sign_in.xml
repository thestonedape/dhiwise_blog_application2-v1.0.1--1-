<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signInVM" type="com.nishantsapplication.app.modules.signin.data.viewmodel.SignInVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSignin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageDhiWiseblackm"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_59pxh"
            android:layout_width="@dimen/_59pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_dhiwiseblackm}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_dhiwiseblackm"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumntitletext"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtTitletext"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{signInVM.signInModel.txtTitletext}"
                style="@style/txtPlantagenetCherokee32" tools:text="@string/lbl_welcome_back"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowgoogleone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageGoogleOne"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_google1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_google1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSigninwithGo"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtSigninwithGo}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_in_with_go"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowfacebookone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageFacebookOne"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_facebook1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_facebook1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSigninwithFa"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtSigninwithFa}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_in_with_fa"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtwitterone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageTwitterOne"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_twitter1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_twitter1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSigninwithTw"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtSigninwithTw}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_in_with_tw"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowappleblacklog"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageAppleblacklog"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_appleblacklog"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_appleblacklog"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSigninwithAp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtSigninwithAp}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_in_with_ap"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowconfirmation"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtConfirmation}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_don_t_have_an_a"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSignup"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtSignup}"
                    style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_sign_up"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
